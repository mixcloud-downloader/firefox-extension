{

  "description": "Download a mix from Mixcloud",
  "manifest_version": 2,
  "name": "Mixcloud Downloader",
  "version": "0.0.2",
  "homepage_url": "http://www.mixcloud-downloader.com/",
  "icons": {
    "48": "icons/logo.svg",
    "96": "icons/logo.svg",
    "128": "icons/logo.svg"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
